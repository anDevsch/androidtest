<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myapplication.sch.example.com.first">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/imgh"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity"
            android:label="Sprlog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity"
            android:launchMode="singleTop"
            android:label="Article list"/>
        <activity android:name=".HttpURLConnectionActivity" />
        <activity android:name=".HttpClientActivity" />
        <activity android:name=".SensorActivity" />
        <activity android:name=".BrowserActivity" />
        <activity android:name=".ArticleActivity"
            android:parentActivityName=".ListViewActivity"
            android:label="Article">
        </activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>